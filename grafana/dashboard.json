{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" },
    { "type": "panel", "id": "heatmap", "name": "Heatmap", "version": "" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "" }
  ],
  "id": null,
  "uid": "supersubtitles",
  "title": "SuperSubtitles",
  "description": "gRPC server, subtitle downloads, and LRU cache metrics",
  "tags": ["supersubtitles", "go", "grpc"],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "templating": {
    "list": [
      {
        "name": "grpc_method",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(grpc_server_started_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}, grpc_method)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "title": "gRPC Server Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "RPC Rate (req/s)",
      "description": "Total rate of gRPC requests started",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "blue", "value": null }] },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(rate(grpc_server_started_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}[$__rate_interval]))",
          "legendFormat": "RPC Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate (%)",
      "description": "Percentage of gRPC calls returning non-OK status",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "100 * sum(rate(grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_code!=\"OK\"}[$__rate_interval])) / clamp_min(sum(rate(grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}[$__rate_interval])), 1)",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Avg Latency",
      "description": "Average RPC handling time",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 2 }
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(rate(grpc_server_handling_seconds_sum{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}[$__rate_interval])) / clamp_min(sum(rate(grpc_server_handling_seconds_count{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}[$__rate_interval])), 1)",
          "legendFormat": "Avg",
          "refId": "A"
        }
      ]
    },
    {
      "title": "P99 Latency",
      "description": "99th percentile RPC handling time",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(grpc_server_handling_seconds_bucket{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}[$__rate_interval])))",
          "legendFormat": "P99",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Active RPCs",
      "description": "Currently in-flight RPCs (started minus handled)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 200 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(grpc_server_started_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}) - sum(grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"})",
          "legendFormat": "In-Flight",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Stream Msgs/s",
      "description": "Rate of streamed messages sent to clients",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "purple", "value": null }] },
          "unit": "cps"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(rate(grpc_server_msg_sent_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\"}[$__rate_interval]))",
          "legendFormat": "Sent/s",
          "refId": "A"
        }
      ]
    },
    {
      "title": "gRPC Request Rate by Method",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "RPC Rate by Method",
      "description": "gRPC request rate per method",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "normal" }
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": { "calcs": ["mean", "max", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "sum by (grpc_method) (rate(grpc_server_started_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"}[$__rate_interval]))",
          "legendFormat": "{{ grpc_method }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate by Method & Code",
      "description": "gRPC errors by method and status code",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" }
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": { "calcs": ["mean", "max", "sum"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "sum by (grpc_method, grpc_code) (rate(grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_code!=\"OK\", grpc_method=~\"$grpc_method\"}[$__rate_interval]))",
          "legendFormat": "{{ grpc_method }} [{{ grpc_code }}]",
          "refId": "A"
        }
      ]
    },
    {
      "title": "gRPC Latency",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Latency Percentiles by Method",
      "description": "P50, P90, P99 handling time per method",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" }
          },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "calcs": ["mean", "max", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, grpc_method) (rate(grpc_server_handling_seconds_bucket{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"}[$__rate_interval])))",
          "legendFormat": "{{ grpc_method }} P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le, grpc_method) (rate(grpc_server_handling_seconds_bucket{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"}[$__rate_interval])))",
          "legendFormat": "{{ grpc_method }} P90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, grpc_method) (rate(grpc_server_handling_seconds_bucket{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"}[$__rate_interval])))",
          "legendFormat": "{{ grpc_method }} P99",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Latency Heatmap",
      "description": "Distribution of RPC handling times",
      "type": "heatmap",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "fieldConfig": { "defaults": {} },
      "options": {
        "calculate": false,
        "yAxis": { "unit": "s" },
        "color": { "scheme": "Oranges", "mode": "scheme" },
        "cellGap": 1,
        "tooltip": { "show": true }
      },
      "targets": [
        {
          "expr": "sum by (le) (increase(grpc_server_handling_seconds_bucket{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"}[$__rate_interval]))",
          "legendFormat": "{{ le }}",
          "refId": "A",
          "format": "heatmap"
        }
      ]
    },
    {
      "title": "gRPC Streaming",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Stream Messages Sent/s by Method",
      "description": "Rate of server-streamed messages per method",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" }
          },
          "unit": "cps"
        }
      },
      "options": {
        "legend": { "calcs": ["mean", "max", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "sum by (grpc_method) (rate(grpc_server_msg_sent_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"}[$__rate_interval]))",
          "legendFormat": "{{ grpc_method }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Avg Messages per RPC",
      "description": "Average number of streamed messages per completed RPC",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showUnfilled": true
      },
      "targets": [
        {
          "expr": "sum by (grpc_method) (rate(grpc_server_msg_sent_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_type=\"server_stream\", grpc_method=~\"$grpc_method\"}[$__rate_interval])) / clamp_min(sum by (grpc_method) (rate(grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_type=\"server_stream\", grpc_method=~\"$grpc_method\"}[$__rate_interval])), 1)",
          "legendFormat": "{{ grpc_method }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "gRPC Status Codes",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Status Code Distribution",
      "description": "Breakdown of gRPC response codes",
      "type": "piechart",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 33 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "OK" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "Internal|Unknown" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "InvalidArgument" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "donut",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum by (grpc_code) (grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"})",
          "legendFormat": "{{ grpc_code }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Per-Method Status Table",
      "description": "Total RPCs handled per method and status code",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 33 },
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "grpc_method" },
            "properties": [{ "id": "displayName", "value": "Method" }]
          },
          {
            "matcher": { "id": "byName", "options": "grpc_code" },
            "properties": [{ "id": "displayName", "value": "Code" }]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [{ "id": "displayName", "value": "Count" }]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Count" }]
      },
      "transformations": [
        {
          "id": "reduce",
          "options": { "reducers": ["lastNotNull"] }
        }
      ],
      "targets": [
        {
          "expr": "sum by (grpc_method, grpc_code) (grpc_server_handled_total{grpc_service=\"supersubtitles.v1.SuperSubtitlesService\", grpc_method=~\"$grpc_method\"})",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ]
    },
    {
      "title": "Subtitle Downloads",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Download Rate (per second)",
      "description": "Rate of subtitle downloads by status",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" }
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*failure.*|.*error.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*success.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "calcs": ["mean", "max", "lastNotNull"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "rate(subtitle_downloads_total[$__rate_interval])",
          "legendFormat": "{{ status }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total Downloads",
      "description": "Cumulative subtitle downloads by status",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 42 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "blue", "value": null }, { "color": "green", "value": 100 }] },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(subtitle_downloads_total)",
          "legendFormat": "Total",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Downloads by Status",
      "description": "Breakdown of downloads by status",
      "type": "piechart",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 42 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*success.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*failure.*|.*error.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
